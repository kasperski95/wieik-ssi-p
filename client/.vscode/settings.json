{
  "cSpell.words": [
    "Nurburgring",
    "TTFB",
    "asynciterable",
    "browserslist",
    "desaturate",
    "dropzone",
    "misano",
    "saturationv"
  ],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "node_modules/": true,
    "package-lock.json": true
  },
  "terminal.integrated.shellArgs.windows": [
    "-NoExit",
    "-Command",
    "${workspaceFolder}/bin/init.ps1"
  ],
  "html.format.wrapLineLength": 80,
  "[typescript]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "[typescriptreact]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  }
}
