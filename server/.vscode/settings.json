{
  "cSpell.words": [
    "Misano",
    "UNPROCESSABLE",
    "nurburgring",
    "postgres",
    "respawn",
    "tsyringe",
    "typeorm",
    "wieik"
  ],
  "terminal.integrated.shellArgs.windows": [
    "-NoExit",
    "-Command",
    "${workspaceFolder}/bin/init.ps1"
  ],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "node_modules": true,
    "package-lock.json": true
  },
  "[typescript]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "[typescriptreact]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  }
}
